{
  "name": "gofakeit",
  "version": "2.3.0",
  "description": "Gofakeit autofill plugin for form field automation",
  "author": "Brian Voelker <brian@webiswhatido.com>",
  "license": "MIT",
  "keywords": [
    "gofakeit",
    "autofill",
    "form",
    "automation",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brianvoe/gofakeit_js.git"
  },
  "type": "module",
  "main": "dist/gofakeit.es.js",
  "module": "dist/gofakeit.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/gofakeit.umd.js",
      "import": "./dist/gofakeit.es.js",
      "default": "./dist/gofakeit.es.js"
    },
    "./umd": "./dist/gofakeit.umd.js",
    "./iife": "./dist/gofakeit.iife.js",
    "./es": "./dist/gofakeit.es.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:docs",
    "build:docs": "node build-docs.js",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --run --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rm -rf dist",
    "release": "node release.js"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.0.0",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}
