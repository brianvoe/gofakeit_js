{
  "name": "gofakeit",
  "version": "1.1.0",
  "description": "Gofakeit autofill plugin for form field automation",
  "author": "Brian Voelker <brian@webiswhatido.com>",
  "license": "MIT",
  "keywords": [
    "gofakeit",
    "autofill",
    "form",
    "automation",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brianvoe/gofakeit_js.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "clean": "rm -rf dist",
    "release": "node release.js"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.0.0",
    "jsdom": "^23.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^1.0.0"
  }
}
